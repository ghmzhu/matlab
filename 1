k=rand(100,3)*100;y=randi(10,100,1);y=y+5;
str=num2str([1:100]');
figure(1); clf
plot3(k(:,1),k(:,2),k(:,3),'o','markersize',1);
hold on
for l=1:100
    plot3(k(l,1),k(l,2),k(l,3),'o','markersize',y(l));
    if y(l)>10
        stem3(k(l,1),k(l,2),k(l,3),'filled')
    else
        stem3(k(l,1),k(l,2),k(l,3))
    end
    text(k(l,1),k(l,2),k(l,3)+1,str(l))
    pause(1)
end
text(k(:,1),k(:,2),k(:,3)+1,str)
>> 
